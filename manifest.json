{
  "name": "Boss Cheating",
  "version": "1.0.7",
  "description": "Press alt + A to activate, alt + S to change the subject (front or back) and ESC to toggle.",
  "homepage_url": "http://stackoverflow.com/",
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["jquery/jquery.js", "jquery/jquery-ui.js", "background.js"],
      "css" : ["jquery/jquery-ui.css", "main.css"]
    }
  ],

  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "manifest_version": 2,
  "icons": { "16": "img/icon.png",
             "48": "img/icon.png",
            "128": "img/icon.png" }
}
